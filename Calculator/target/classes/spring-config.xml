<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



    <bean id="bootstrap" class="ua.goit.java.bootstrap.Bootstrap">
        <property name="taskProvider" ref="taskProvider"/>
        <property name="executorFactory" ref="executorFactory"/>
    </bean>

    <bean id="taskProvider" class="ua.goit.java.taskProvider.IntegerTaskProvider" init-method="init"/>

    <bean id="SerialExecutor" class="ua.goit.java.executor.SerialExecutor"/>

    <bean id="executorFactory" class="ua.goit.java.ExecutorFactory.ExecutorFactory">

        <lookup-method name="getIntegerExecutor" bean="SerialExecutor"/>
    </bean>
    <bean id="Calculator" class="ua.goit.calculator.Calculator"/>

</beans>